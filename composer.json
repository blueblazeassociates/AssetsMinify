{
  "name" : "blueblazeassociates/assetsminify",
  "description" : "WordPress Plugin for assets management and optimization",
  "type" : "wordpress-plugin",
  "homepage" : "https://wordpress.org/plugins/assetsminify/",
  "license" : "GPL-2.0+",
  "keywords" : [
    "assets",
    "coffeescript",
    "compass",
    "CSS",
    "JS",
    "less",
    "minify",
    "sass"
  ],
  "replace": {
    "wpackagist-plugin/assetsminify" : "2.0.2"
  },
  "include-path" : [
    "src/"
  ],
  "autoload" : {
    "psr-0" : {
      "" : "src/"
    }
  },
  "require" : {
    "kriswallsmith/assetic" : "1.2.*",
    "leafo/lessphp" : "0.4.*",
    "leafo/scssphp" : "0.1.*",
    "ptachoire/cssembed" : "1.0.*",
    "blueblazeassociates/scssphp-compass" : "0.0.*",
    "patchwork/jsqueeze" : "1.0.*",
    "mrclay/minify" : "2.2.*",
    "monolog/monolog" : "1.13.*",
    "blueblazeassociates/monolog-parser" : "0.0.*"
  },
  "require-dev" : {
    "phpunit/phpunit" : "3.7.*"
  },
  "repositories" : [{
      "type" : "composer",
      "url" : "http://composer.blueblaze.co"
    }
  ]
}
