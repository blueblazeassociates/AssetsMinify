{
  "name" : "blueblazeassociates/assetsminify",
  "description" : "WordPress Plugin for assets management and optimization",
  "type" : "wordpress-plugin",
  "homepage" : "https://wordpress.org/plugins/assetsminify/",
  "license" : "GPL-2.0+",
  "keywords" : [
    "assets",
    "coffeescript",
    "compass",
    "CSS",
    "JS",
    "less",
    "minify",
    "sass"
  ],
  "include-path" : [
    "src/"
  ],
  "autoload" : {
    "psr-0" : {
      "" : "src/"
    }
  },
  "require" : {
    "kriswallsmith/assetic" : "1.2.*",
    "leafo/lessphp" : "0.4.*",
    "leafo/scssphp" : "dev-master",
    "ptachoire/cssembed" : "1.0.*",
    "leafo/scssphp-compass" : "dev-master",
    "patchwork/jsqueeze" : "1.0.*",
    "mrclay/minify" : "2.2.*",
    "monolog/monolog" : "dev-master",
    "pulse00/monolog-parser" : "0.0.*"
  },
  "require-dev" : {
    "phpunit/phpunit" : "3.7.*"
  }
}
